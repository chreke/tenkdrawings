<h1>Sign in</h1>
<% flash.each do |key, value| %>
  <div><%= value %></div>
<% end %>
<%= form_for :session, url: sessions_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
<% end %>
<p>Don't have an account? <%= link_to "Sign up now!", new_user_path %></p> 
